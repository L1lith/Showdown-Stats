{
    "name": "Showdown Stats",
    "description": "Allows you to view stats on Pokemon Showdown",
    "version": "1.1.7",
    "icons": {
    "48": "icons/48.png"
    },
    "permissions": ["*://pokemonshowdown.com/*", "*://*.pokemonshowdown.com/*"],
    "content_scripts": [{
      "matches": ["*://pokemonshowdown.com/*", "*://*.pokemonshowdown.com/*"],
      "js": ["launch.js"],
      "css": ["styles.css"]
    }],
    "web_accessible_resources" : ["build.js"],
    "manifest_version": 2
}
